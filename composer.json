{
    "name": "enjoyscms/core",
    "description": "EnjoysCMS Core",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/annotations": "^1.11",
        "doctrine/migrations": "^3.6",
        "doctrine/orm": "^2.8",
        "enjoys/assets-collector": "^2.0",
        "enjoys/config": "^1.0",
        "enjoys/cookie": "^2.0",
        "enjoys/functions": "^2.0",
        "enjoys/session": "^2.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "php-di/php-di": "^7.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "ramsey/uuid": "^4.1",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/finder": "^6.3",
        "symfony/http-foundation": "^6.3",
        "symfony/routing": "^6.3",
        "symfony/psr-http-message-bridge": "^2.1",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "infection/infection": "^0.26.16",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^5.4",
        "dg/bypass-finals": "^1.4",
        "fakerphp/faker": "^1.21",
        "guzzlehttp/guzzle": "^7.5.0",
        "symfony/var-dumper": "^6.3"
    },
    "scripts": {
        "psalm": "./vendor/bin/psalm --show-info=true --config=psalm.xml",
        "tests": "XDEBUG_MODE=coverage ./vendor/bin/phpunit",
        "infection": [
            "Composer\\Config::disableProcessTimeout",
            "XDEBUG_MODE=coverage  ./vendor/bin/infection -j2"
        ]
    },
    "autoload": {
        "psr-4": {
            "EnjoysCMS\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\EnjoysCMS\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
